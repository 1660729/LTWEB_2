<div class="card shadow mt-3">
    <h5 class="card-header">
          TÀI KHOẢN
    </h5>
    <table class="table table-hover">
    <thead>
        <tr>
            <th scope="col">ID </th>
            <th scope="col">Người Dùng</th>
            <th scope="col">Họ Tên</th>
            <th scope="col">Bút Danh</th>
            <th scope="col">Email</th>
            <th scope="col" class="text-right">
                {{!-- <a class="btn btn-outline-success btn-xs" href="/categories/add" role="button" title="Add">
                    <i class="fa fa-plus" aria-hidden="true"></i>
                    Thêm chuyên mục
                </a> --}}
            </th>

        </tr>
    </thead>
    <tbody>
        {{#each this.nguoidung}}
        <tr>
            <th scope="row">{{ID}}</th>
            <td>{{NguoiDung}}</td>
            <td>{{HoTen}}</td>
            <td>{{ButDanh}}</td>
            <td>{{Email}}</td>
            <td class="text-right">
                <a name="" id="" class="btn btn-primary btn-xs" href="/account/edit/{{ID}}" role="button" title="Edit">
                    <i class="fa fa-pencil" aria-hidden="true"></i>
                </a>    
            </td>
        </tr>
        {{/each}}
    </tbody>
</table>
</div>

